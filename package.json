{
  "name": "plebdev-bench",
  "version": "0.1.0",
  "description": "Local LLM benchmark runner",
  "type": "module",
  "bin": {
    "bench": "./src/index.ts"
  },
  "scripts": {
    "pb": "bun run src/index.ts run",
    "bench": "bun run src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
